@using System.IO;
@using Westwind.AspNetCore.Markdown
@{
    ViewData["Title"] = "Home Page";
}

@if (System.IO.File.Exists((string)ViewData["MarkdownFile"]))
{
    @Markdown.ParseHtmlStringFromFile((string)ViewData["MarkdownFile"])
}
else
{
    @Markdown.ParseHtmlString("# File not found.  \n## [Home](/)")
}
